alias v='nvim';




# Set colors as found from alacritty, based on guesstimate from defaults
set -U fish_color_command blue;
set -U fish_color_autosuggestion brblack;
set -U fish_color_comment green;
set -U fish_color_cwd red;
set -U fish_color_cwd_root brred;
set -U fish_color_end normal;
set -U fish_color_error darkred;#red --dim;
set -U fish_color_escape magenta;
set -U fish_color_command blue --dim --bold;
set -U fish_color_host bryellow --bold;
set -U fish_color_match red --dim --background=red;
set -U fish_color_normal normal;
set -U fish_color_operator brblue;
set -U fish_color_param cyan;
set -U fish_color_redirection magenta;
set -U fish_color_search_match --background=blue;
set -U fish_color_selection white --bold --backgorund=brblack;
set -U fish_color_status red --dim;
set -U fish_color_user brmagenta;
set -U fish_color_valid_path yellow;
set -U fish_color_color_completion cyan;
set -U fish_color_color_description normal;
set -U fish_color_color_prefix blue --dim;
set -U fish_color_color_progress black --background=yellow;
